{
  "name": "kapa/xsdvalidator",
  "version": "0.0.2",
  "description": "Validate XML against XSD in Javascript",
  "main": "dist/xsdvalidator.js",
  "dependencies": {
    "requirejs": "latest",
    "underscore": "latest",
    "wgxpath": "https://wicked-good-xpath.googlecode.com/files/wgxpath.install.js",
    "xsd": "bazmegakapa/kapa-xsd",
    "XsdLibrary": "bazmegakapa/kapa-XsdLibrary",
    "objTools": "bazmegakapa/kapa-objTools"
  },
  "install": {
    "path": {
      "js": "js/lib"
    },
    "sources" : {
      "wgxpath": {
        "mapping": [
          { "bower_components/wgxpath/index.js": "wgxpath.js" }
        ]
      },
      "requirejs-text" : "bower_components/requirejs-text/text.js"
    }
  },
  "keywords": [
    "xml",
    "xsd",
    "validation"
  ],
  "authors": [
    "Kapa <kapa@sipaksi.net>"
  ],
  "license": "GPLv3",
  "homepage": "http://github.com/bazmegakapa/xsdval",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
